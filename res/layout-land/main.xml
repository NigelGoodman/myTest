<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >



    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_weight="1.03" >

        <Button
            android:id="@+id/A"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:onClick="onClick"
            android:text="A" />



        <Button
            android:id="@+id/B"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_above="@+id/A"
            android:layout_alignLeft="@+id/A"
            android:layout_marginLeft="39dp"
            android:onClick="onClick"
            android:text="B" />

        <Button
            android:id="@+id/C"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/B"
            android:layout_toRightOf="@+id/A"
            android:onClick="onClick"
            android:text="C" />



        <Button
            android:id="@+id/D"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignLeft="@+id/B"
            android:layout_below="@+id/A"
            android:onClick="onClick"
            android:text="D" />



        <Button
            android:id="@+id/E"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/A"
            android:layout_toLeftOf="@+id/D"
            android:onClick="onClick"
            android:text="E" />



        <Button
            android:id="@+id/F"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_above="@+id/E"
            android:layout_toLeftOf="@+id/A"
            android:onClick="onClick"
            android:text="F" />



        <Button
            android:id="@+id/G"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_above="@+id/A"
            android:layout_toLeftOf="@+id/D"
            android:onClick="onClick"
            android:text="G" />



        <Button
            android:id="@+id/H"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignBaseline="@+id/B"
            android:layout_alignBottom="@+id/B"
            android:layout_toRightOf="@+id/B"
            android:onClick="onClick"
            android:text="H" />



        <Button
            android:id="@+id/I"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/H"
            android:layout_toRightOf="@+id/C"
            android:onClick="onClick"
            android:text="I" />



        <Button
            android:id="@+id/J"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/C"
            android:layout_toRightOf="@+id/D"
            android:onClick="onClick"
            android:text="J" />



        <Button
            android:id="@+id/K"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/F"
            android:layout_toLeftOf="@+id/E"
            android:onClick="onClick"
            android:text="K" />



        <Button
            android:id="@+id/L"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_above="@+id/K"
            android:layout_toLeftOf="@+id/F"
            android:onClick="onClick"
            android:text="L" />



        <Button
            android:id="@+id/M"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignBaseline="@+id/G"
            android:layout_alignBottom="@+id/G"
            android:layout_alignLeft="@+id/K"
            android:onClick="onClick"
            android:text="M" />



        <Button
            android:id="@+id/N"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_above="@+id/I"
            android:layout_toRightOf="@+id/H"
            android:onClick="onClick"
            android:text="N" />



        <Button
            android:id="@+id/O"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/N"
            android:layout_toRightOf="@+id/I"
            android:onClick="onClick"
            android:text="O" />



        <Button
            android:id="@+id/P"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignLeft="@+id/N"
            android:layout_below="@+id/I"
            android:onClick="onClick"
            android:text="P" />



        <Button
            android:id="@+id/Q"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_below="@+id/L"
            android:layout_toLeftOf="@+id/K"
            android:onClick="onClick"
            android:text="Q" />



        <Button
            android:id="@+id/R"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_above="@+id/Q"
            android:layout_toLeftOf="@+id/L"
            android:onClick="onClick"
            android:text="R" />



        <Button
            android:id="@+id/S"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignBaseline="@+id/M"
            android:layout_alignBottom="@+id/M"
            android:layout_alignLeft="@+id/Q"
            android:onClick="onClick"
            android:text="S" />

    </RelativeLayout>

</LinearLayout>